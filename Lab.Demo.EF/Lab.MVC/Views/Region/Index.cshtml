@model List<Lab.MVC.Models.RegionView>

@{
    ViewBag.Title = "Region Page";
}

<h1>Hola soy el Index de Region</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Descrition</th>
        </tr>
    </thead>
    
    <tbody>

    @foreach (var region in Model)
    {
        <tr>
            <th>@region.Id</th>
            <td>@region.Description</td>
        </tr>
    }

    </tbody>

</table>